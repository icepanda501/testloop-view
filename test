	<script type="text/javascript">
		var workCount = 1; eduCount = 1;
		$(document).ready(function() {
			
			$('.Apply-Now-Modal-Link').click(function() {
				$('.modal-background').fadeIn();
			});
			$('.close-modal').click(function() {
				$('.modal-background').fadeOut();
			});
		    
		    $( "#work_start1" ).datepicker();
		    $( "#work_finish1" ).datepicker();
		    $( "#edu_start1" ).datepicker();
		    $( "#edu_finish1" ).datepicker();

		});
		$('#add_more_experience').click(function(){
			workCount = workCount + 1;
			$('#work_experience').append('<label for="Company-Name" class="field-label signup">Work Experience:</label>'+
					'<input id="work_company'+workCount+'" type="text" placeholder="Company Name" name="work_company['+workCount+']" data-name="Company Name" class="w-input" onfocus="companyAutoComplete(\'work_company'+workCount+'\', \'company_biztype'+workCount+'\');" >' +
					'<input id="company_biztype'+workCount+'" type="hidden" name="company_biztype['+workCount+']" value="1">' +
					'<input id="work_position'+workCount+'" type="text" placeholder="Your Position" name="work_position['+workCount+']" data-name="Your Position" class="w-input" >' +
					'<input id="work_start'+workCount+'" type="text" placeholder="From DD / MM / YYYY" name="work_start['+workCount+']" data-name="Work Start" class="w-input date-from" >' +
					'<input id="work_finish'+workCount+'" type="text" placeholder="To DD / MM / YYYY" name="work_finish['+workCount+']" data-name="Work Finish" class="w-input date-to" >' +
					'<textarea id="work_description'+workCount+'" placeholder="Tell us about your work here" name="work_description['+workCount+']" data-name="Work Description" class="w-input"></textarea>'
				);
			$( "#work_start"+workCount ).datepicker();
			$( "#work_finish"+workCount ).datepicker();
		});
		$('#add_more_education').click(function(){
			eduCount = eduCount + 1;
			$('#edu_history').append('<label for="University-Name" class="field-label signup">Education:</label>'+
					'<input id="edu_college'+eduCount+'" type="text" placeholder="University Name" name="edu_college['+eduCount+']" data-name="University Name" class="w-input"  onfocus="collegeAutoComplete(\'edu_college'+eduCount+'\', \'college_location'+eduCount+'\');" >'+
					'<input id="college_location'+eduCount+'" type="hidden" name="college_location['+eduCount+']" value="138">'+
					'<input id="edu_major'+eduCount+'" type="text" placeholder="Concentration" name="edu_major['+eduCount+']" data-name="Concentration" class="w-input" >'+
					'<input id="edu_start'+eduCount+'" type="text" placeholder="From DD / MM / YYYY" name="edu_start['+eduCount+']" data-name="Work Start" class="w-input date-from" >'+
					'<input id="edu_finish'+eduCount+'" type="text" placeholder="To DD / MM / YYYY" name="edu_finish['+eduCount+']" data-name="Work Finish" class="w-input date-to" >'+
					'<textarea id="edu_description'+eduCount+'" placeholder="Tell us about your class here" name="edu_description['+eduCount+']" data-name="Education Description" class="w-input"></textarea>'
				);
			$( "#edu_start"+eduCount ).datepicker();
		    $( "#edu_finish"+eduCount ).datepicker();
		});

		$("#basic_submit,#history_submit").click(function(){
			$(".error").hide();
			var hasError = false;
	        var passwordVal = $("#resume_password1").val();
	        var checkVal = $("#resume_password2").val();
	        var phone = $("#resume_phone").val();
	        var resume_email = $("#resume_email").val();

	        if (checkVal == '') {
	        	hasError == true;
	        } else if (passwordVal != checkVal) {
	        	// alert('Passwords do not match');
	            $("#label_password2").after('<span class="error"><label style="color:red;">Passwords do not match.<label></span>');
	            $('html, body').animate({ scrollTop: $("#label_password2").offset().top }, 'slow');
	            hasError = true;
	        }

	       	$.ajax({

                method: "POST",
                url:"checkDupEmail",
                data : { resume_email : resume_email},
                async : false,
                success : function(data) {
                    if(data == "Duplicate email"){
                    	$("#label_email").after('<span class="error"><label style="color:red;">Email already exists<label></span>');
                    	$('html, body').animate({ scrollTop: $("#label_email").offset().top }, 'slow');
	     		       hasError = true;
                    }
                }
            });

			return !hasError;



	     

		});
		var baseurl = "<?php echo base_url(); ?>";
        $('#userfile').change(function() {
            var data = new FormData();
            jQuery.each(jQuery('#userfile')[0].files, function(i, file) {
                data.append('userfile', file);
            });
            $.ajax({
                method: "POST",
                url: baseurl+"welcome/upload_prepic",
                data : data,
                cache: false,
                contentType: false,
                processData: false,
                beforeSend: function(xhr, settings) {
                    $('#userPhoto').attr('src',baseurl+'static/img/loading.gif');
                },
                success : function(data, textStatus, XMLHttpRequest) {
                    if(data == "not pass"){

                        $('#userPhoto').attr('src',baseurl+'uploads/anonymous.jpg');
                        alert("can't upload this image , please check your image");

                    }else{
                        $('#userPhoto').attr('src',baseurl+'uploads/'+data);
                        $('#nameFile').val(data);
                    }
                }
            });
        });  
	</script>
	<script type="text/javascript">
		function companyAutoComplete(id_company, id_biztype) {
	        $("#"+id_company).value = 'eiei';
	        var availableTags = [];
	        var works_biztype = [];
	        <?php 
	            $arrCompanyStatus['company_status'] = 'ENABLED';
	            $poolcompany = $this->mol_work->getAllCompany($arrCompanyStatus);
	            $allCompany['company'] = $poolcompany['result'];
	            foreach ($allCompany['company'] as $key => $value) {
	            ?>
	                availableTags.push(<?php echo json_encode($value['company_name']); ?>);
	                works_biztype.push(<?php echo json_encode($value['biztype_name']); ?>);
	            <?php
	            }
	        ?>
	        // console.log(id_company);
	        
	        $( "#"+id_company ).autocomplete({
	            selectFirst: true,
	            autoFocus: true,
	            source: availableTags,
	            select: function(event,ui) {
	                var company_index = availableTags.indexOf(ui.item.value);
	                if (company_index < 0) {
	                    // console.log('no index in availableTags');
	                    document.getElementById(id_biztype).disabled = false;
	                } else {
	                    document.getElementById(id_biztype).disabled = false;
	                    var company_id = availableTags.indexOf(ui.item.value);
	                    var company_type = works_biztype[company_id];
	                    document.getElementById(id_biztype).value = company_type;
	                    // console.log(id_biztype+"_hidden = "+document.getElementById(id_biztype).value);
	                }
	            }
	        });
	    }

	    function collegeAutoComplete(id_college, id_location) {
	        var availableTags = [];
	        var edu_location = [];
	        var edu_location_ids = [];
	        var location = "";
	        <?php
	            $allCollege = $this->mol_education->getAllCollege();
	            foreach ($allCollege['college'] as $key => $value) {
	            ?>
	                availableTags.push(<?php echo json_encode($value['college_name_en']); ?>);
	                edu_location.push(<?php echo json_encode($value['city_name'].", ".$value['country_name']); ?>);
	                edu_location_ids.push(<?php echo json_encode($value['location_id']); ?>);
	            <?php
	            }
	        ?>

	        $( "#"+id_college ).autocomplete({
	            selectFirst: true,
	            autoFocus: true,
	            source: availableTags,
	            select: function(event,ui) {

	                var college_index = availableTags.indexOf(ui.item.value);
	                // console.log("college_index = "+college_index);
	                if (college_index < 0) {
	                    // console.log('no index in availableTags');
	                    document.getElementById(id_location).disabled = false;
	                } else {
	                    document.getElementById(id_location).disabled = false;
	                    var college_id = availableTags.indexOf(ui.item.value);
	                    var college_location_id = edu_location_ids[college_id];
	                    document.getElementById(id_location).value = edu_location[college_id];
	                }
	            }
	        });
	    }
	</script>
